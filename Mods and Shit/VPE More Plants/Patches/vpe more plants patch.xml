<?xml version="1.0" encoding="utf-8"?>
<Patch>

  <!-- vegetable milk -->
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Vanilla Cooking Expanded</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationReplace">
          <success>Always</success>
          <xpath>Defs/ResearchProjectDef[defName="VCE_VegMilkExtraction"]/label</xpath>
          <value>
            <label>vegetable milk extractor</label>
          </value>
        </li>
      </operations>
    </match>
  </Operation>


</Patch>
